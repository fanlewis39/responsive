            if($(document).scrollTop() >= 260 ){  
                $(".menu").addClass("menu_down");
                $(".dropdown-toggle").addClass("dropdown-toggle_down");
            }else{
                $(".menu").removeClass("menu_down");
                $(".dropdown-toggle").removeClass("dropdown-toggle_down");
            }
        }) 
    </script>
    <script type="text/javascript">
        $.ajax({
            url: './article1.html',
            dataType: 'html',
            success: function (data) {
                var parser = new DOMParser();
                var doc = parser.parseFromString(data, 'text/html');
                var title1 = doc.querySelector("h2").innerText;
                var p1 = doc.querySelector("p").innerText;
                $(".row1_h > h2").text(title1);
                $(".row1_h > p").text(p1);
            }
        })
    </script>
    <script type="text/javascript">
